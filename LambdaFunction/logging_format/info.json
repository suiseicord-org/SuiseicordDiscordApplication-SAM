{
    "version": 1,
    "formatters": {
      "detailed": {
        "class": "logging.Formatter",
        "format": "%(asctime)s [%(levelname)-8s] %(name)s: %(message)s",
        "datefmt": "%Y-%m-%d %H:%M:%S"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "formatter": "detailed"
      },
      "test": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "formatter": "detailed"
      }
    },
    "loggers": {
      "test": {
        "handlers": ["console"]
      }
    },
    "root": {
      "level": "INFO",
      "handlers": ["console"]
    }
}